<?php

include('../src/Article.php');

class ArticleTest extends PHPUnit_Framework_TestCase {
	public function setUp() {
		
	}
	
	/* get() */
	public function testGetReturnsArticleWithContent() {
		$article = Article::get('test_article.html');
		$this->assertStringEqualsFile('test_article.html', $article->getContent());
	}
}
